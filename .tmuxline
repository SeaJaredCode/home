# This tmux statusbar config was created by tmuxline.vim
# on Fri, 12 May 2017

set -g status-bg "colour234"
set -g message-command-fg "colour144"
set -g status-justify "centre"
set -g status-left-length "100"
set -g status "on"
set -g pane-border-fg "colour237"
set -g pane-border-bg "colour0"
set -g pane-active-border-fg "colour27"
set -g pane-active-border-bg "colour0"
set -g message-bg "colour237"
set -g status-right-length "100"
set -g status-right-attr "none"
set -g message-fg "colour144"
set -g message-command-bg "colour237"
set -g status-attr "none"
set -g status-utf8 "on"
set -g status-left-attr "none"
setw -g window-status-fg "colour27"
setw -g window-status-attr "none"
setw -g window-status-activity-bg "colour234"
setw -g window-status-activity-attr "underscore"
setw -g window-status-activity-fg "colour27"
setw -g window-status-separator ""
setw -g window-status-bg "colour234"

set -g status-left "#[fg=colour15,bg=colour27] #S #[fg=colour27,bg=colour237]#[fg=colour144,bg=colour237] #W  #[fg=colour237,bg=colour234]#[fg=colour27,bg=colour234] [#P]:#20T "
set -g status-right "#[fg=colour27,bg=colour234] %a #[fg=colour234,bg=colour237]#[fg=colour144,bg=colour237] %R #[fg=colour237,bg=colour27]#[fg=colour15,bg=colour27] #H "
setw -g window-status-format "#[default] [#I]:#W "
setw -g window-status-current-format "#[fg=colour234,bg=colour237]#[fg=colour144,bg=colour237] [#I]:#W #[fg=colour237,bg=colour234]"

# Sans powerline
set -g status-left "#[fg=colour15,bg=colour27] #S #[fg=colour144,bg=colour237] #W #[fg=colour27,bg=colour234] [#P]:#20T "
set -g status-right "#[fg=colour27,bg=colour234] %a #[fg=colour144,bg=colour237] %R #[fg=colour15,bg=colour27] #H "
setw -g window-status-format "#[default] [#I]:#W "
setw -g window-status-current-format "#[fg=colour144,bg=colour237] [#I]:#W "

